<div class="container">
    <div class="row">
        <div class="col-xs-12 text-center" id="top-bar">
            <h1>Fantasy Edge Player Sorting</h1>
        </div>
        <div class="col-xs-9 text-center">
            <div class="row">
                {{#each player_data }}
                    <div class="col-xs-7 text-center">
                        <pre>
                            {{this.Player}}
                        </pre>
                    </div>
                    <div class="col-xs-4 text-center">
                        <pre>
                            {{this.WS}}
                        </pre>
                    </div>
                {{/each}}
            </div>
        </div>
    </div>
</div>