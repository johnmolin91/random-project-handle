<div class="container">
    <div class="row">
        <div class="col-xs-12 text-center" id="top-bar">
            <h1>Fantasy Edge Player Sorting</h1>
        </div>
        <div class="col-xs-4 text-center">
            <div class="row">
                {{#each player_data }}
                    <div class="col-xs-8 text-right">
                         {{this.Player}}
                    </div>
                    <div class="col-xs-4 text-left">
                        {{this.rawAdv}}
                    </div>
                {{/each}}
            </div>
        </div>
        <div class="col-xs-4 text-center">
            <div class="row">
                {{#each schedule_data}}
                    <div class="col-xs-12 text-center">
                        {{this.visitorTeam}}
                        {{this.homeTeam}}
                    </div>
                {{/each}}
	</div>
        </div>
    </div>
</div>